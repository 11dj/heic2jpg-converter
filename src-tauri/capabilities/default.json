{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "fs:allow-read-file",
    "fs:allow-read-dir",
    "fs:allow-write-file",
    "fs:allow-create",
    "fs:allow-app-write",
    "fs:allow-temp-write",
    "dialog:allow-open",
    "dialog:allow-save"
  ]
}
